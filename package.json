        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "googleapis": "^144.0.0",
        "jest": "^29.7.0",
        "knex": "^3.1.0",
        "log4js": "^6.9.1",
        "node-cron": "^3.0.3",
        "objection": "^3.1.5",
        "pg": "^8.13.1",
        "zod": "^3.24.1"
    },
    "devDependencies": {
