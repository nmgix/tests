        "migrate:rollback": "knex migrate:rollback --knexfile ./src/db/sql/config.ts",
        "lint": "eslint \"src/**/*.ts\""
    },
    "imports": {
        "#*": [
            "./src/*"
        ]
    },
    "license": "ISC",
