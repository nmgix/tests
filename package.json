    "version": "1.0.0",
    "main": "src/index.ts",
    "scripts": {
        "build": "tsc -p . --noEmit false",
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
        "test": "jest",
        "migrate:make": "knex migrate:make --knexfile ./src/db/sql/config.ts",
